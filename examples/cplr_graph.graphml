<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
         http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">

  <!-- Node attributes -->
  <key id="kind" for="node" attr.name="kind" attr.type="string"/>
  <key id="source" for="node" attr.name="source" attr.type="string"/>
  <key id="authority" for="node" attr.name="authority" attr.type="string"/>
  <key id="file_path" for="node" attr.name="file_path" attr.type="string"/>
  <key id="line_number" for="node" attr.name="line_number" attr.type="int"/>

  <!-- Edge attributes -->
  <key id="dep_type" for="edge" attr.name="type" attr.type="string"/>
  <key id="edge_file_path" for="edge" attr.name="file_path" attr.type="string"/>
  <key id="edge_line_number" for="edge" attr.name="line_number" attr.type="int"/>

  <graph id="ny_cplr_sample" edgedefault="directed">

    <node id="date_filed">
      <data key="kind">variable</data>
      <data key="source">user_input</data>
    </node>
    <node id="date_served">
      <data key="kind">variable</data>
      <data key="source">user_input</data>
    </node>
    <node id="opposing_party_served">
      <data key="kind">variable</data>
      <data key="source">derived</data>
    </node>
    <node id="court_jurisdiction">
      <data key="kind">variable</data>
      <data key="source">user_input</data>
    </node>
    <node id="motion_type">
      <data key="kind">variable</data>
      <data key="source">user_input</data>
