    <edge id="e15" source="service_complete" target="opposing_party_served">
      <data key="dep_type">implicit</data>
      <data key="edge_file_path">examples/ny_cplr_sample.yaml</data>
      <data key="edge_line_number">29</data>
    </edge>
    <edge id="e16" source="date_served" target="opposing_party_served">
