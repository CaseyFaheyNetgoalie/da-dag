      <data key="edge_file_path">examples/ny_cplr_sample.yaml</data>
      <data key="edge_line_number">63</data>
    </edge>
    <edge id="e4" source="motion_filed_on_time" target="motion_requirements_rule">
      <data key="dep_type">implicit</data>
      <data key="edge_file_path">examples/ny_cplr_sample.yaml</data>
      <data key="edge_line_number">70</data>
    </edge>
    <edge id="e5" source="proof_of_service_filed" target="motion_requirements_rule">
